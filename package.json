{
  "name": "training-diary-api",
  "version": "0.3.0",
  "description": "Training diary for saving information about your progress",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js",
    "test": "export NODE_ENV=test && mocha server/**/*.test.js",
    "test-watch": "nodemon --exec 'npm test'",
    "test-cc": "export NODE_ENV=test && nyc -r html mocha server/**/*.test.js",
    "build": "del-cli frontend/public/js/app && webpack --config frontend/webpack.config.dev.js --progress --profile --watch",
    "watch": "nodemon server/server.js & del-cli frontend/public/js/app && webpack --config frontend/webpack.config.dev.js --progress --profile --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scholtysek-software/training-diary-api.git"
  },
  "keywords": [
    "training",
    "diary"
  ],
  "engines": {
    "node": "8.2.1",
    "npm": "5.3.0"
  },
  "author": "Patryk Scholtysek <patryk@scholtysek-software.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/scholtysek-software/training-diary-api/issues"
  },
  "homepage": "https://github.com/scholtysek-software/training-diary-api#readme",
  "dependencies": {
    "@angular/animations": "^4.0.0",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/platform-server": "^4.0.0",
    "@angular/router": "^4.0.0",
    "@angular/upgrade": "^4.0.0",
    "cookie-parser": "~1.4.3",
    "body-parser": "1.17.2",
    "hbs": "~3.1.0",
    "morgan": "~1.6.1",
    "express": "4.15.4",
    "lodash": "4.17.4",
    "mongodb": "2.2.31",
    "mongoose": "4.11.7",
    "core-js": "^2.4.1",
    "rxjs": "^5.2.0",
    "zone.js": "^0.8.5",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "@types/core-js": "0.9.36",
    "@types/node": "^6.0.45",
