    "angular2-template-loader": "^0.5.0",
    "awesome-typescript-loader": "^3.1.2",
    "del-cli": "^0.2.0",
    "eslint": "3.18.0",
    "eslint-config-airbnb-base": "11.1.2",
    "eslint-plugin-import": "2.2.0",
    "expect": "1.20.2",
    "html-loader": "^0.4.4",
    "mocha": "3.5.0",
    "nodemon": "1.11.0",
    "nyc": "11.1.0",
    "raw-loader": "^0.5.1",
    "supertest": "3.0.0",
    "ts-loader": "^2.0.3",
    "typescript": "^2.1.4",
    "webpack": "^2.2.1",
    "webpack-merge": "^4.1.0"
  }
}
